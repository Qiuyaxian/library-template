    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.5",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "file-save": "^0.2.0",
    "husky": "^8.0.1",
    "inquirer": "7.0.0",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "postcss": "^8.4.17",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "stylelint": "^14.13.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "^28.0.0",
