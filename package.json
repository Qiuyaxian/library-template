{
  "name": "library-template",
  "version": "1.0.0",
  "description": "",
  "main": "lib/library-template.common.js",
  "scripts": {
    "new": "node build/new/index.js",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "build:entry": "node build/create-entry.js",
    "build:common": "webpack --config build/webpack.common.js",
    "build:component": "webpack --config build/webpack.component.js",
    "build:theme": "node build/theme.js && gulp build --gulpfile ./src/theme/gulpfile.js && cp-cli ./src/theme/lib/ lib/theme",
    "build:analyzer": "cross-env use_analyzer=true npm run build:component",
    "dist": "npm run clean & npm run build:entry & npm run build:common & npm run build:components & npm run build:theme",
    "docs:serve": "npm run build:entry & npx vuepress dev docs",
    "docs:build": "npm run build:entry & vuepress build docs",
    "lint:prettier": "prettier --write --list-different --loglevel warn \"**/*.{js,json,css,less,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.{vue,less}\"",
    "commit": "git-cz",
    "postInstall": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@laomao800/vue-demo-box": "^1.1.7",
    "vue": "^2.7.10"
  },
